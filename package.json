{
  "name": "roots",
  "version": "3.0.0-rc.1",
  "author": "Jeff Escalante <hello@jenius.me>",
  "contributors": [
    "Sam Saccone <sam@samx.it>"
  ],
  "description": "simple, flexible, and powerful static site compiler",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jenius/roots"
  },
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "man": "man",
    "doc": "doc"
  },
  "dependencies": {
    "coffee-script": "1.7.x",
    "colors": "0.6.x",
    "readdirp": "0.3.x",
    "minimatch": "0.2.x",
    "lodash": "2.x",
    "js-yaml": "3.x",
    "when": "2.x",
    "accord": "0.0.6",
    "mkdirp": "0.3.x",
    "optimist": "0.6.x",
    "update-notifier": "0.1.x",
    "chokidar": "0.8.x",
    "open": "0.0.4",
    "connect": "2.x",
    "sprout": "0.0.7",
    "configstore": "0.2.x",
    "infestor": "0.2.x",
    "faye-websocket": "0.7.x",
    "graceful-fs": "2.x",
    "vinyl": "0.2.x"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*",
    "glob": "3.x",
    "rimraf": "2.x",
    "coveralls": "2.x",
    "mocha-lcov-reporter": "0.0.1",
    "istanbul": "0.2.x"
  },
  "scripts": {
    "test": "mocha",
    "coverage": "make build; istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | rm -rf ./coverage; make unbuild",
    "coveralls": "make build; istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage; make unbuild"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
